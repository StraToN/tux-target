<?xml version="1.0" standalone="no" ?>
<!-- this is a mtp-target gui resource -->
<resource>
	<object class="CGuiFrame" name="menuFrame">
		<alignment>eAlignCenterHorizontal|eAlignCenterVertical</alignment>
		<minSize>0,80</minSize>
		<position>0,50</position>
		<element>
			<object class="CGuiVBox" name="vbox">
				<alignment>eAlignExpandHorizontal|eAlignExpandVertical</alignment>
				<element>
					<object class="CGuiButton" name="bHowToPlay">
						<minSize>150,0</minSize>
						<element>
							<object class="CGuiText" name="txtHowToPlay">
								<string> How To Play </string>
							</object>
						</element>
					</object>
					<object class="CGuiButton" name="bPlayOnline">
						<minSize>150,0</minSize>
						<element>
							<object class="CGuiText" name="txtPlayOnline">
								<string> Play Online </string>
							</object>
						</element>
					</object>
					<object class="CGuiButton" name="bPlayOnlan">
						<minSize>150,0</minSize>
						<element>
							<object class="CGuiText" name="txtPlayOnlan">
								<string> Play on lan </string>
							</object>
						</element>
					</object>
					<!--
					<object class="CGuiButton" name="bPlayOnline">
						<minSize>150,0</minSize>
						<element>
							<object class="CGuiText" name="txtPlayOnlan">
								<string> Connect </string>
							</object>
						</element>
					</object>
					-->
					<object class="CGuiButton" name="bExit">
						<minSize>150,0</minSize>
						<element>
							<object class="CGuiText" name="btextExit">
								<string>Exit</string>
							</object>
						</element>
					</object>
				</element>
			</object>
		</element>
	</object>
</resource>
