<?xml version="1.0" standalone="no" ?>
<!-- this is a mtp-target gui resource -->
<resource>
	<object class="CGuiFrame" name="checkingFrame">
		<minSize>400,0</minSize>
		<!-- <position>100,100</position> -->
		<alignment>eAlignCenterVertical|eAlignCenterHorizontal</alignment>
		<element>
			<object class="CGuiVBox" name="vbox">
				<alignment>eAlignExpandHorizontal|eAlignExpandVertical</alignment>
				<element>
					<object class="CGuiText" name="checkingMessage">
						<alignment>eAlignExpandHorizontal|eAlignExpandVertical</alignment>
						<string>Please wait while checking : </string>
					</object>
					<object class="CGuiText" name="checkingFilename">
						<!-- <alignment>eAlignExpandHorizontal|eAlignExpandVertical</alignment> -->
						<string>file.tga</string>
					</object>
				</element>
			</object>
		</element>
	</object>
</resource>
